<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Don't backup any app data -->
        <exclude domain="database" path="." />
        <exclude domain="file" path="." />
        <exclude domain="sharedpref" path="." />
    </cloud-backup>

    <device-transfer>
        <!-- Don't include in device transfers -->
        <exclude domain="database" path="." />
        <exclude domain="file" path="." />
        <exclude domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>